.lvmap{
  width: 100%;
  height: 50vh;
}

.lvfang{
  background: #fff;
  display: inline-block;
  padding:2px 10px;
  text-align: center;
  border-radius: 5px;
  position: absolute;
  right: 10px;
  top: 10px;
  box-shadow: 1px 1px 1px rgba(0,0,0,0.3);
}



.lvfang_jian,.lvfang_jia{
  padding:0 5px;
  font-size: 30px;
  font-weight: bold;
  line-height: 60px;
  min-height: 60px;
  
}

.lvfang_jian{
  font-size: 35px;
}

.biglv{
  height: 50vh;
}


.lvone{
  min-height: 50px;
  padding: 10px;
  border-bottom: 1px dashed #ccc;
}

.h1{
  overflow: hidden;
  
}

.h1 .title{
  font-size: 16px;
  font-weight: bold;
  color: #333;
}

.h1 .tel{
  margin-top: 5px;
  font-size: 14px;
}

.add{
  font-size: 14px;
  margin-top:3px;
  color: #999;
}


.page-img{
  float: left;
  border:1px solid #ccc;
  margin-right: 10px;
}

.page-title{
  padding:2px  10px;  
}



.page-img image{
  width: 100px;
  height: 100px;
}


.get-map{
  position: absolute;
  left: 10px;
  top: 10px;
  font-weight: bold;
  border: 1px solid #ccc;
  background: #fff;
  color: #333;
  border-radius: 100px;
  text-align: center;
  line-height: 20px;
  width: 40px;
  white-space: normal;
  height: 50px;
  font-size: 16px;
  padding: 0 10px;
  padding-top: 10px;
}