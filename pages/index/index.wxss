/**index.wxss**/


.header{
  background: #F9F9F9;
  display: flex;
}

.search{
  flex: 1;
 
}

.collect{
  width: 80px;
  text-align: center;
  align-items: center;
}

.collect image{
  width: 100rpx;
  height: 100rpx;
  border-radius: 100rpx;
  border: 1px solid #ededed;
}

.collect-flex{
  width: 100rpx;
  display: block;
  margin: auto;
  position: relative;
  text-align: center;
}


.header{
 padding: 10rpx 0;
}

.text{
  color: #fff;
  background: #FC552F;
  border-radius: 1rem;
  padding: 2rpx 1rpx;
  font-size: .65rem;
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
}


.search{
 
  background: #fff;
  border: 1rpx solid #DADADA;
  border-radius: 15rpx;
  margin: 0 20rpx;
}

.sear-box{
  display: flex;
  align-items: stretch;
  height: 100%;
}

.sear-box icon{
  width: 70rpx;
  position: relative;
  top: 34rpx;
  left: 20rpx;
}


.input-place{
  color: #ccc;
}

.sear-box input{
  flex: 1;
  height: 100%;
  line-height: 100%;
  font-size: 35rpx;
}



.banner-img{
  width: 100%;
  height: 100%;
}

.banner-swiper{
  height: 200px;
}


.list{
  padding: 20rpx 20rpx;
}

.title-h1{
  font-size: 40rpx;
  border-left: 10rpx solid #FC552F;
  padding-left: 10rpx;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}

.list-one{
  font-size: 35rpx;
  line-height: 45rpx;
  padding: 20rpx;
  background: #fff;
  border-radius: 10rpx;
  margin-bottom: 20rpx;
}

.index-scroll-view{
  height: 50vh;
  overflow: hidden;
}

.hover{
  background: #ededed;
  box-shadow: 1px 1px 1px rgba(0, 0, 0, 0.5), -1px -1px 1px rgba(255, 255, 255, 0.5);
}




.page-footer{
   height: 100rpx;
   background:url('data:image/png;base64,') no-repeat center 92vh;
  margin-bottom: 20rpx;
  background-attachment: fixed;
  background-size: auto 50rpx;
}